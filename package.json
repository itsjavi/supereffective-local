{
  "name": "supereffective-dex-tracker",
  "version": "1.3.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist/* && vite build",
    "build:production": "NODE_ENV=production && vite build && rm -rf _site && mkdir -p _site && mv ./dist/* ./_site",
    "chlog": "pnpm make:changelog",
    "codegen": "pnpm codegen:svg",
    "codegen:svg": "rm -rf src/lib/icons && svgr --no-prettier --out-dir='src/lib/icons' --typescript --icon=24 --ext=tsx ./src/assets/icons",
    "dev": "vite --host",
    "format": "pnpm lint:fix",
    "lint": "tsc --noEmit && pnpm lint-prettier && pnpm lint-biome",
    "lint-biome": "biome lint . --max-diagnostics 50",
    "lint-biome-ci": "biome ci . --max-diagnostics 50",
    "lint-prettier": "prettier --check \"src/**/*.{md,json,css,scss}\"",
    "lint:ci": "pnpm lint-prettier && pnpm lint-biome-ci",
    "lint:fix": "pnpm lint:fix-prettier && pnpm lint:fix-package-json && pnpm lint:fix-biome",
    "lint:fix-biome": "biome check --max-diagnostics 50 --apply .",
    "lint:fix-package-json": "sort-package-json",
    "lint:fix-prettier": "prettier  --log-level warn --write \"src/**/*.{md,json,css,scss}\"",
    "make": "pnpm make:component",
    "make:changelog": "vite-node src/scripts/make-changelog-entry.ts",
    "make:component": "vite-node src/scripts/make-component.ts",
    "make:data": "pnpm make:optimized-data",
    "make:images": "pnpm make:optimized-images",
    "make:optimized-data": "vite-node src/scripts/make-optimized-data.ts && pnpm lint:fix-prettier",
    "make:optimized-images": "vite-node src/scripts/make-optimized-images.ts",
    "precommit": "CI=true pnpm test && CI=true pnpm lint",
    "prepare": "husky install",
    "preview": "pnpm run start",
    "start": "vite preview --host",
    "tauri": "tauri",
    "test": "jest --env=jsdom",
    "test:coverage": "jest --env=jsdom --coverage && open coverage/index.html",
    "version:bump": "pnpm changelogen --bump --release",
    "version:publish": "pnpm version:release",
    "version:release": "git push && git push --tags"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.17.15",
    "@tauri-apps/api": "^1.5.3",
    "clsx": "^2.1.0",
    "open-props": "^1.6.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.2",
    "@supeffective/dataset": "^2.2.2",
    "@svgr/cli": "^8.1.0",
    "@swc/core": "^1.3.104",
    "@swc/jest": "^0.2.29",
    "@tauri-apps/cli": "^1.5.9",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/express": "^4.17.21",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "changelogen": "^0.5.5",
    "cssnano": "^6.0.3",
    "express": "^4.18.2",
    "html-minifier-terser": "^7.2.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.33",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-import": "^16.0.0",
    "postcss-import-ext-glob": "^2.1.1",
    "postcss-jit-props": "^1.0.14",
    "prettier": "^3.2.4",
    "puppeteer": "^21.7.0",
    "rollup": "^4.9.5",
    "sass": "^1.69.7",
    "sharp": "^0.33.2",
    "sort-package-json": "^2.6.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-node": "^1.2.1",
    "vite-plugin-html-prerender": "^0.2.0",
    "vite-plugin-pwa": "^0.17.4"
  }
}